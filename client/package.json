{
  "name": "frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "$(yarn bin)/ng",
    "start": "$(yarn bin)/ng serve --proxy-config proxy.conf.json",
    "start:hmr": "$(yarn bin)/ng serve --hmr --configuration=hmr --proxy-config proxy.conf.json",
    "build": "$(yarn bin)/ng build",
    "build:prod": "$(yarn bin)/ng build --prod",
    "test": "$(yarn bin)/ng test",
    "lint": "$(yarn bin)/ng lint",
    "e2e": "$(yarn bin)/cypress run --recordVideo false",
    "e2e:manual": "$(yarn bin)/cypress open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.2",
    "@angular/common": "^7.0.2",
    "@angular/compiler": "^7.0.2",
    "@angular/core": "^7.0.2",
    "@angular/forms": "^7.0.2",
    "@angular/http": "^7.0.2",
    "@angular/platform-browser": "^7.0.2",
    "@angular/platform-browser-dynamic": "^7.0.2",
    "@angular/router": "^7.0.2",
    "core-js": "^2.4.1",
    "ngx-bootstrap": "^3.1.1",
    "patternfly-ng": "^4.5.1",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.0.0-rc.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.3",
    "@angular/cli": "^7.0.3",
